}
.form .txt98,.txt98 {
    width: 98px;
}
.form .txt118,.txt118 {
    width: 118px;
}
.form .txt128,.txt128 {
    width: 118px;
}
.form .txt168,.txt168 {
    width: 168px;
}
.form .txt198,.txt198 {
    width: 198px;
}
.form .txt398,.txt398 {
    width: 398px;
}
.form .txt438,.txt438 {
    width: 438px;
}
.form .txt448,.txt448 {
    width: 448px;
}



/*下拉列表*/

.pulldown-box {
    position: relative;
    width: 228px;
    height: 28px;
    line-height: 28px;
    border: 1px solid #ccc;
    z-index: 1;
}

.pulldown-box .pulldown {
    height: 100%; 
    cursor: pointer;
    background: #fff;
    font-size: 12px;
    color: #333;
    text-indent: 8px;
    outline: none;
}

.pulldown-box .pulldown span {
    float: left;
}

.pulldown-box .pulldown i {
    float: right;
    display: inline;
    margin-right: 10px;
    color: #333;
    font-style: normal;
}

.pulldown-box .pulldown-list {
    position: absolute;
    width: 228px;
    background: #fff;
    border: 1px solid #ccc;
    left:-1px;
    top: 27px;
    z-index: 1;
    display: none;
}

.pulldown-box .pulldown-list li {
    font-size: 12px;
    line-height: 24px;
    color: #000;
    padding: 0 7px;
    cursor: pointer;
}

.pulldown-box .pulldown-list li:hover {
    background: #ccc;
}

.pulldown-box.w98{
    width: 98px;
}
.pulldown-box.w118{
    width:118px;
}
.pulldown-box.w98{
    width: 98px;
}
.pulldown-box.w98{
    width: 98px;
}
.pulldown-box.w98{
    width: 98px;
}

/*表格列表（在售商品商品列表）*/
.table-list{padding-bottom: 73px;}
.table-list table{border:1px solid #dbdada;}
.table-list-hd{background:#fff4e8;height: 30px;}
.table-list-hd td{color:#707070;font-weight: bold;text-align:center;}
.col1{width:38%}
.col2{width:17%}
.col3{width:15%}
.col4{width:12%}
.col5{width:18%}
.table-list-bd table{background: #fff;width: 100%;height: 93px;margin-bottom: 10px;}
.table-list-bd table td{border:1px solid #edecec;text-align: center;}
.table-list-bd .detail-wrap{text-align: left;margin: 0 10px 0 19px;}
.table-list-bd .detail-wrap input{margin-right: 12px;}
.table-list-bd .p-img,.table-list-bd .p-msg{display: inline-block;*display:inline;*zoom:1;vertical-align: middle;}
.table-list-bd .p-img img{width: 61px;height: 61px;}
.table-list-bd .p-msg{width:214px;line-height: 18px}

/*main search*/
.main-search{background: #fff;border:1px solid #dbdada;height:58px;line-height:58px;color: #333;padding:0 0 0 10px;}
.main-search .form .txt{height: 28px;line-height: 28px\9;margin-top: 14px;*margin-top:0;float: none;*vertical-align: middle;}
.main-search .form .txt38{text-align:center;text-indent: 0;}
.main-search .pulldown-box{margin-top: 14px;}
.main-search dd{margin-right: 25px;*margin-right:12px;}
.main-search .grey-btn{width: 50px;}
.main-search p{margin-left:3px;}

/*全选的区域*/
.operate-all {
    line-height: 42px;
    color: #707070;
    padding-left: 20px;
}

.operate-all label {
    margin-right:24px;
}

/*分页*/
.page{height:42px;line-height: 42px;color: #707070;vertical-align:top;}
.page input{margin-top: 8px;}
.page a{padding: 0 6px;}
.page a:hover,.page .current{color:#bf0008;}
.page-link a{vertical-align: top;line-height: 42px;display: inline-block;}
.page .grey-btn{width: 50px;}
.page .txt{text-align:center;text-indent: 0;margin-top: 0;height:23px;line-height: 23px\9;}
.page .total-page{margin-left: 23px;}
.page .go-page{margin: 0 10px 0 18px;letter-spacing:3px;line-height: 42px;vertical-align: middle;}
.page .go-page input{margin-right: 3px;float: none;*display: inline-block;*vertical-align: middle;}
.page .confirm{*margin-top:-1px;}

/*上传按钮*/
.file_con{position: relative;}
.file_con .upload_btn{width:93px; height:36px;opacity:0; filter:alpha(opacity=0);position: absolute;left:0;top:0;}
