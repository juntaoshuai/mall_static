	<script>
		$(function(){
			// 导航条展开
			$(".nav-list-content").hover(function(){
				$(this).find(".all-list-content").show();
			},function(){
				$(this).find(".all-list-content").hide();
				$('.all-right-content .all-r-item').hide();
				$('.all-right-content').hide();
				$(".all-items .active").removeClass('active');
			})

            // 支付方式切换
            $(".pay-style .mall-icon").click(function(){
                var index = $(this).index();

                $(".pay-style .active").removeClass('active');

                $(this).addClass('active');
            });

			//小导航条展开
			$(".all-items h4").mouseover(function(){
				var index  = $(this).parent().index();
				$(".all-items .active").removeClass('active');
				$(this).addClass('active');
				$('.all-right-content').show();
				$('.all-right-content .all-r-item').hide();
				$('.all-right-content .all-r-item').eq(index).show();
			})
		})
	</script>
