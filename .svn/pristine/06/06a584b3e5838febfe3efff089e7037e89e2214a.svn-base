/*插入代码以及显示插入的代码需用到的样式*/
.toolbar
{
	font-family: 微软雅黑;
    background: none repeat scroll 0 0 rgba(247, 242, 242, 0.59) !important;
    border: 1px solid #ddd !important;
    color: rgb(52, 170, 52) !important;
    font-size: 1em !important;
    height: auto !important;
	padding: 2px 10px;
    /*padding: 5px 10px !important;*/
    position: static !important;
    width: auto !important;
}
.toolbar span
{
    display: inline !important; /*margin-right: 1em !important;*/
    /*line-height: 150%;*/
    visibility: visible;
	height:30px;
	line-height:30px;
	margin-left:10px;
}
.toolbar span a
{
    text-decoration: none;
}
.font_info
{
    font-size: 16px;
    font-weight: bold;
}
.code_init_none
{
    width: 100%;
    height: auto;
    display: none;
}
.code_init_block
{
    width: 100%;
    height: auto;
    display: block;
}
select {
	width: 310px;
	height: 30px;
	line-height: 30px;
	padding: 4px 5px;
	border: 1px solid #ddd;
	outline: none;
}
.ke-code-type3{
	padding: 4px 5px;
	border: 1px solid #ddd;
	outline: none;
}