/*订单管理*/
.tabs{border-bottom: 2px solid #bf0008;padding-left: 20px;}
.tabs li{width: 79px;height:28px;line-height: 28px;border:1px solid #dbdada;border-bottom:none;color:#666;float: left;text-align: center;margin-right: 8px; background-color: #f7f7f7; cursor:pointer;}
.tabs li.active{background-color: #c00;color:#fff;font-weight: bold;border-color: #bf0008;}


.tabs-con .all{line-height: 42px;color:#707070;padding-left: 20px;}
.tabs-con .all label{margin-right: 36px;}



.orderlist-hd{background-color: #fff4e8;border:1px solid #dbdada;}
.orderlist-hd li{font-size: 12px;color:#707070;font-weight: bold;line-height: 28px;float: left;text-align: center;}
.w346{width: 346px;}
.w110{width: 110px;}
.w79{width: 79px;}
.w144{width: 144px;}
.w115{width: 115px;}
.w129{width: 129px;}




.orderlist{margin-top: 10px;}
.orderlist li{border:1px solid #dbdada;background-color: #fff;color:#707070;}
.orderlist .profile{background-color: #f4f6f9;height: 30px;line-height: 30px;overflow: hidden;}
.orderlist .profile .user{padding-left: 19px;}
.orderlist .profile .user i{font-size: 20px;color:#005eae;}
.orderlist .detail,.orderlist .unit-price,.orderlist .quantity,.orderlist .collection,.orderlist .status,.orderlist .opera{border-right: 1px solid #edecec;height: 91px;text-align: center;}
.orderlist .detail{text-align:left;}
.orderlist .detail-wrap{margin:15px 5px 0 19px;}
.orderlist .p-name{line-height:16px;color:#005eae;margin-bottom: 2px;}
.orderlist .p-name a{color:#005eae;}
.orderlist .p-name a:hover,.orderlist .trade-img a:hover{color:#c00;}
.orderlist .p-img{margin-left: 19px;}
.orderlist .p-img,.orderlist .p-img img{width:61px;height: 61px;}
.orderlist .p-msg{margin-left:108px;}
.orderlist .trade-img a{color:#ff8033;}
.orderlist .refer{height: 100%;vertical-align: middle; display: inline-block;}


.orderlist .unit-price{line-height:91px;}
.orderlist .quantity{line-height:91px;}
.orderlist .collection,.orderlist .status{line-height: 28px;}
.orderlist .collection p,.orderlist .status p{ display: inline-block;vertical-align: middle; }
.orderlist .opera{border-right:none;}
.status1{color:#ff9633;}
.status2{color:#005eae;}
.status3{color:#bf0008;}

/*首次登录*/
.reglogin-wrap{background: #f3f3f3;margin-bottom: 60px;}



/*first login*/
.first-login{padding-bottom: 93px;padding-top:35px;}
.first-login h2{font:bold 20px/32px microsoft Yahei;border-bottom:1px solid #bf0008;color:#bf0008;display: inline-block;}
.first-login .login-tips{line-height: 12px;margin: 20px 0 29px;color:#797979;}

.ml283{margin-left: 283px;}

.form .form-hd{width:390px;text-align: center;}
/*login*/
.login{padding:99px 0 103px;}
.login .find-pwd{width:250px;text-align: right;line-height: 30px;clear: both;}
.login .find-pwd a{color:#005eae;}
.login .find-pwd a:hover{color:#c00;}
/*pwd update success*/
.pwd-success{padding:52px 0 93px;}
.icon-success{font-size: 36px;color:#c81623;font-weight: normal;}
.pwd-success .pwd-tips{height:36px;line-height: 36px;font-size: 14px;color:#c81623;font-weight: bold;}
.pwd-success .form-hd{margin-bottom: 58px;}
/*find pwd*/
.find-pwd-wrap{border-top: 2px solid #bf0008;margin-top: 26px;}
.find-pwd-wrap .reglogin-wrap{width: 920px;margin:60px auto;border-top: 1px solid #acacac;padding-bottom: 176px;}
.find-pwd-wrap .reglogin-wrap .findpwd-hd{font:20px/36px microsoft Yahei;color:#000;margin: 32px 0 0 133px;}
.find-pwd-wrap .reglogin-wrap .form{margin-left: 220px;}
.form label .txt118{width:118px;}
.getcode{width:98px;height: 26px;border: 1px solid #ccc; background:#f7f7f7;background:-moz-linear-gradient(top, #fcfcfc, #ededed);
    background:-webkit-linear-gradient(top, #fcfcfc, #ededed);
    background:-ms-linear-gradient(top, #fcfcfc, #ededed);
    background:linear-gradient(top, #fcfcfc, #ededed);outline: none;border-radius: 2px;font-size: 12px;color:#005eae;}