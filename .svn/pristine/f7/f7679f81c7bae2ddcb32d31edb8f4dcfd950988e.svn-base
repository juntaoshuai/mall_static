 body{font-family: simsun,Arial;font-size: 12px;background-color: #fff;}
 a{color:#666;}
 a:hover{color:#c00;}
 .c0{color:#000;}
.white-bg{background: #fff;}
 .mt13{margin-top: 13px;}
 .lh20{line-height: 20px;}
 .lh22{line-height: 22px;}
 .lh24{line-height: 24px;}
/*字体图标*/
@font-face {
  font-family: 'iconfont';
  src: url('http://at.alicdn.com/t/font_1472606083_029907.eot'); /* IE9*/
  src: url('http://at.alicdn.com/t/font_1472606083_029907.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('http://at.alicdn.com/t/font_1472606083_029907.woff') format('woff'), /* chrome、firefox */
  url('http://at.alicdn.com/t/font_1472606083_029907.ttf') format('truetype'), /* chrome、firefox、opera、Safari, Android, iOS 4.2+*/
  url('http://at.alicdn.com/t/font_1472606083_029907.svg#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont{font-family:"iconfont";font-style:normal;vertical-align: middle;}                     

.main-wrapper{width:1100px;margin-left:auto;margin-right: auto;}
#side{width: 150px;height:592px;float: left;background-color: #fff;}
#main{width:930px;float:right;}
/*header*/
.login-bar{background: #f1f1f1;height: 30px;line-height: 30px;}
.login-bar .welcome{text-decoration: none;}
.login-bar .home{font-size: 20px;color: #6b6b6b;margin-right: 9px;}

#header{background: #fff;}
#header h1{font:24px/68px microsoft Yahei;color:#4f4f4f;background: url(../images/title_line.png) left center no-repeat;padding-left: 15px;}

#nav{border-bottom: 2px solid #bf0008;}
#nav ul{background-color: #bf0008;padding-left: 60px;}
#nav ul li{float: left; height: 38px;line-height: 38px;margin-right: 56px;}
#nav ul li a{color: #fff; font-size: 14px;font-weight: bold;}
/*面包屑导航*/
.position{color:#666;line-height: 52px;}
.position span,.position a{color:#666;margin:0 6px;}
.position a:hover{color:#c00;}

/*left nav*/
.leftnav li{height: 25px;line-height: 25px;margin-bottom: 15px;}
.leftnav li a{display:block;font:12px/25px microsoft Yahei;color:#303131;padding-left:37px;}
.leftnav li a:hover,.leftnav li a.active{background:url(../images/nav_hover.png) right center no-repeat;font-weight:bold;color:#fff;text-decoration: none;}

/*footer*/
#footer{text-align: center;padding-bottom:10px;background-color: #fff;}
.service-mall{background-color: #f7f9f9;line-height: 140px;}
.service-mall span{ background: url(../images/mall_sprite.png) 0 0 no-repeat;position:relative; display: inline-block;*display: inline;*zoom:1; height: 50px; font:18px/50px "微软雅黑";color:#212121; padding-left: 63px; padding-right: 27px;margin-right: 28px;}
.service-mall .item1{background-position: 0 -33px;}
.service-mall .item2{background-position: 0 -83px;}
.service-mall .item3{background-position: 0 -133px;}
.service-mall .item4{background-position: 0 -183px;padding-right: 0;margin-right: 0;}
.service-mall span i{ position: absolute;width:1px; height: 45px;right:0;top:4px; background: #d7d7d7;}
#footer .footer-nav a{color:#666;margin:0 28.5px;}
#footer .copyright,#footer .copyright a{color:#999;}
#footer .copyright a:hover{color:#c00;}

/*title*/
.page-title{background-color: #fff;height: 40px;line-height: 40px;}
.page-title h2{border-left: 2px solid #bf0008;font-family: microsoft Yahei;font-size: 20px;color:#000;padding-left: 7px;}

/*btn*/
.redbtn{width:93px;height: 36px;background-color: #c81623;font:bold 16px/36px microsoft Yahei;color:#fff;border:none;border-radius: 3px;outline:none;}

/*表单*/
.form label,.form .btn-box{margin:0 0 20px;vertical-align: top;line-height: 35px;float: left;}
.form label.mb10{margin-bottom: 10px;}
.form label .item-l,.form label .item-r,.form .btn-box .item-l,.form .btn-box .item-r{float: left;display: inline;}
.form label .item-l,.form .btn-box .item-l{font-size: 14px;line-height: 35px;color: #313131;width:84px;text-align: right;margin-right:14px;}
.form label .txt{width:365px;height:33px;line-height:33px\9;border:1px solid #ccc;background: #fff;font-size: 14px;color: #313131;text-indent:8px;outline: none;}
.form label .txt{width:248px;float:left;font-family: simsun;}
.item-r{color: #fff;}
.item-r .zk{color: #ff1d1d;}
.item-r .fptype span{margin:0 35px 0 9px;}
.item-r .fptype input{vertical-align: middle;}


/*下拉列表*/
.pulldown-box{position: relative;width:367px;height: 28px;z-index:1;}
.pulldown-box .pulldown{width:365px;height:28px;line-height:28px\9;cursor: pointer;border:1px solid #ccc;background: #fff;font-size: 14px;color: #313131;text-indent:8px;outline: none;}
.pulldown-box .pulldown span{float: left;}
.pulldown-box .pulldown i{width:12px;height: 16px;float: right; display:inline;background: url(../images/ico-down.png) 0 0 no-repeat;margin: 5px 5px 0 0;}
.pulldown-box .pulldown-list{position: absolute;width:365px;background: #111;border:1px solid #29c2dc;left:0;top:27px;z-index:1; display: none;}
.pulldown-box .pulldown-list li{font-size: 14px;color: #fff; padding: 0 7px; cursor: pointer;}
.pulldown-box .pulldown-list li:hover{background: #29c2dc;}